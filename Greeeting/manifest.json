{
    "name": "Greeting ",
    
    "version": "1.0",
    
    "description": "This Chrome extension greets the user each time they open a new tab",
    
    "manifest_version": 1.0,

    "incognito": "split",
    
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    
    "permissions": [
       "activeTab"
     ]
  }